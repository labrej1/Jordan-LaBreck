
public class Program {

	public static void main(String[] args) {
		
		for (int i = 1; i <= 5; i++) {
			for (int s = 0; s < i; s++) {
				System.out.print("*");
			}
			System.out.println();
		}
		
		for (int i = 5; i >= 1; i--) {
			for (int s = 0; s < i; s++) {
				System.out.print("*");
			}
			System.out.println();
		}
	}

}

import java.util.Scanner;

public class Program {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		System.out.println("What is your name?: ");
		String name = "a";
		
		name = sc.nextLine();
		System.out.println(name + ", " + name + " Bo-b" +(name.substring(name.length()-5)));
		System.out.println("Bo-na-na fanna fo-f" +(name.substring(name.length()-5)));
		System.out.println("Fee fi mo-m"+(name.substring(name.length()-5))+ ", " + name);
		
	}

}

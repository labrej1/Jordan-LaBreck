import java.util.Random;

public class Program {

	public static void main(String[] args) {
		int value = 17;
		Random rng = new Random();
		value = rng.nextInt(75)+1;
		
		if(value <= 15)
			System.out.print("B ");
		else if(value <= 30)
			System.out.print("I ");
		else if(value <= 45)
			System.out.print("N ");
		else if(value <= 60)
			System.out.print("G ");
		else
			System.out.print("O ");
		
		System.out.println(value);
	}

}
